//Statement Execution

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int a;
    float b;
    char c,d[100],e[100];
    scanf("%d %f %c %s %[^\n]s",&a,&b,&c,d,e);
    printf("Integer is %d\n",a);
    printf("Float is %.2f\n",b);
    printf("Character is %c\n",c);
    printf("Strings are %s and %s",d,e);
    return 0;
}
