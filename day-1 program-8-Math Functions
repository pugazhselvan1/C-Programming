//Math Functions

#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    float a;
    int x,y,z;
    scanf("%f",&a);
    scanf("%d",&x);
    scanf("%d",&y);
    scanf("%d",&z);
    printf("%d\n",(int)floor(a));
    printf("%d\n",(int)ceil(a));
    printf("%d\n",(int)sqrt(x));
    printf("%d",(int)pow(y,z));
    
    return 0;
}
